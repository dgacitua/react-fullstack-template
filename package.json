{
  "name": "react-fullstack-template",
  "version": "1.0.0",
  "description": "A template for Fullstack webapps based on React + Node.js",
  "main": "./server/index.js",
  "scripts": {
    "start": "node ./server/index.js",
    "client-build": "webpack --mode production --output ./dist/client/index.js",
    "server-build": "babel ./server -d ./dist/server",
    "build": "npm run client-build && npm run server-build",
    "client-dev": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server-dev": "babel-watch ./server/index.js",
    "dev": "concurrently \"npm run server-dev\" \"npm run client-dev\"",
    "clean": "rimraf ./dist"
  },
  "author": "Daniel Gacitua",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-watch": "^2.0.7",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.0",
    "nodemon": "^1.17.5",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.0.1",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/react-fontawesome": "0.0.20",
    "bootstrap": "^3.3.7",
    "express": "^4.16.3",
    "jquery": "^3.3.1",
    "react": "^16.4.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.0"
  }
}
