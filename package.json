{
  "name": "react-fullstack-template",
  "version": "1.0.2",
  "description": "A template for Fullstack webapps based on React + Node.js",
  "main": "./server/index.js",
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=production node ./dist/server/index.js",
    "client-build": "webpack --mode production --output ./dist/client/index.js",
    "server-build": "babel ./server -d ./dist/server",
    "build": "npm run client-build && npm run server-build",
    "client-dev": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server-dev": "babel-watch ./server/index.js",
    "dev": "concurrently \"npm run server-dev\" \"npm run client-dev\"",
    "clean": "rimraf ./dist"
  },
  "author": "Daniel Gacitua",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-watch": "^7.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^2.1.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.0.1",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "bootstrap": "^4.3.1",
    "express": "^4.16.3",
    "jquery": "^3.3.1",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.6",
    "react-dom": "^16.8.6"
  }
}
